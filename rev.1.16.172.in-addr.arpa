;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@       IN      SOA     jajal. root.jajal. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;

@       IN      NS      ns1.jajal.com.
33      IN      PTR     jajal.com.

; 33 -> ini IP nya jajal.com.
